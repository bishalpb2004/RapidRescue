<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".ui.home.HomeFragment">

    <ScrollView
        android:fillViewport="true"
        android:id="@+id/r0s7zfjcnane9"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#FF0F0F0F"
        android:paddingTop="22dp"
        android:paddingBottom="85dp"
        android:paddingHorizontal="10dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:orientation="horizontal"
                android:background="#000000"
                android:layout_width="match_parent"
                android:layout_height="130sp">

                <LinearLayout
                    android:id="@+id/linear_layout1"
                    android:layout_width="wrap_content"
                    android:layout_height="120sp"
                    android:background="@color/black"
                    android:paddingVertical="29dp"
                    android:paddingHorizontal="26dp"
                    android:layout_marginBottom="75dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/emergencyDel"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_marginBottom="8dp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="welcome"
                        />
                    <TextView
                        android:id="@+id/textview_name"
                        android:textColor="#00E6C5"
                        android:textSize="18sp"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Bishal P Biswas"
                        />
                </LinearLayout>


            </LinearLayout>
            <LinearLayout
                android:id="@+id/linear_layout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:layout_marginTop="10sp"
                android:paddingVertical="25dp"
                android:paddingHorizontal="60dp"
                android:layout_marginBottom="10dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/r9zpivou11li"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="8dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/rbf8l6l86wbe"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="12dp"
                        android:src="@drawable/img_1"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/edit_num"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="Edit"
                        />
                </LinearLayout>
                <TextView
                    android:id="@+id/SOS_message"
                    android:textColor="#00E6C5"
                    android:textSize="18sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="SOS Message"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/linear_layout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:paddingVertical="25dp"
                android:paddingHorizontal="60dp"
                android:layout_marginBottom="10dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rri7rtfgtszn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="9dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/r4ogol5l4mi"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="11dp"
                        android:src="@drawable/img_2"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/add_em_num"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="add"
                        />
                </LinearLayout>
                <TextView
                    android:id="@+id/emergency"
                    android:textColor="#00E6C5"
                    android:textSize="18sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="emergency"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/linear_layout5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:paddingVertical="29dp"
                android:paddingHorizontal="61dp"
                android:layout_marginBottom="11dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rnms68ai5ysa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="11dp"
                    android:orientation="horizontal">
                    <RelativeLayout
                        android:id="@+id/rg717hcypvsv_rl"
                        android:layout_width="21dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="12dp"
                        >
                        <ImageView
                            android:id="@+id/rg717hcypvsv"
                            android:layout_width="21dp"
                            android:layout_height="21dp"
                            android:src="@drawable/img_4"
                            android:scaleType="fitXY"
                            />
                    </RelativeLayout>
                    <TextView
                        android:id="@+id/view_em_num"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="view"
                        />
                </LinearLayout>
                <TextView
                    android:id="@+id/instructions"
                    android:textColor="#00E6C5"
                    android:textSize="18sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Emergency Numbers"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/knowInstructions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:paddingVertical="27dp"
                android:paddingHorizontal="62dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/rnog4xmm7tym"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/rcyrf28ctp0r"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:layout_marginEnd="14dp"
                        android:src="@drawable/img_5"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/rsrfxi6zxa2"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_weight="1"
                        android:text="know the"
                        />
                </LinearLayout>
                <TextView
                    android:id="@+id/rg2ul3j9ltyp"
                    android:textColor="#00E6C5"
                    android:textSize="18sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Instructions"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>